{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "outputDirectory": "public",
  "installCommand": "composer install --no-dev --optimize-autoloader",
  "buildCommand": "composer install --no-dev --optimize-autoloader",
  "env": {
    "APP_ENV": "production",
